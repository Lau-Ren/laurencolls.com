{
  "name": "laurencolls.com",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "dev:start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push",
    "postinstall":  "browserify . -o bundle.js -g uglifyify",
    "start": "node server.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lau-ren/laurencolls.com"
  },
  "author": "lau-ren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lau-ren/laurencolls.com/issues"
  },
  "homepage": "https://github.com/lau-ren/laurencolls.com",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "path": "^0.12.7",
    "react-router": "^2.5.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
